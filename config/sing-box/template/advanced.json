        "JP",
        "SG",
        "KR"
      ]
    },
    {
      "tag": "Twitter",
      "type": "selector",
      "outbounds": [
        "HK",
        "US",
        "JP",
        "SG",
        "KR"
      ]
    },
    {
      "tag": "Final",
      "type": "selector",
      "outbounds": ["Bypass","DIRECT"],
      "default": "Bypass"
    },
    
    {
      "tag": "HK",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "JP",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "US",
      "type": "selector",
      "outbounds": []
