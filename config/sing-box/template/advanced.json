{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    }
  },
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8",
        "detour": "Bypass"
      },
      {
        "tag": "tb",
        "address": "https://223.5.5.5/dns-query",
        "detour": "DIRECT"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "tb",
        "disable_cache": true
      },
      {
        "inbound": "tun",
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip",
        "rewrite_ttl": 1
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "google"
      },
      {
        "rule_set": "geosite-cn",
        "server": "tb"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "server": "block"
      }
    ],
    "final": "google",
    "independent_cache": true
  },
  "inbounds": [
    {
      "tag": "tun",
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2081
        }
      }
    },
    {
      "tag": "mixed",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2081,
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "tag": "Bypass",
      "type": "selector",
      "outbounds": ["Automatic","Manual"],
      "default": "Automatic"
    },
    {
      "tag": "Automatic",
      "type": "urltest",
      "outbounds": []
    },
    {
      "tag": "Manual",
      "type": "selector",
      "outbounds": ["DIRECT"]
    },
    {
      "tag": "Apple",
      "type": "selector",
      "outbounds": ["DIRECT","Bypass"]
    },
    {
      "tag": "Google",
      "type": "selector",
      "outbounds": [
        "HK",
        "JP",
        "US",
        "KR",
        "SG"
      ]
    },
    {
      "tag": "GitHub",
      "type": "selector",
      "outbounds": ["Bypass","DIRECT"]
    },
    {
      "tag": "Microsoft",
      "type": "selector",
      "default": "DIRECT",
      "outbounds": [
        "HK",
        "SG",
        "US",
        "DIRECT"
      ]
    },
    {
      "tag": "OpenAI",
      "type": "selector",
      "outbounds": [
        "JP",
        "US",
        "KR",
        "SG"
      ]
    },
    {
      "tag": "Spotify",
